<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/colorWhite"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.55" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="50dp"
            android:fontFamily="@font/bmjua"
            android:text="안녕하세요"
            android:textColor="@color/MainColor"
            android:textSize="20dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="SpUsage" />

        <TextView
            android:id="@+id/userName_textview_drawer"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:text=""
            android:textColor="@color/MainColor"
            android:textSize="22dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/textView5"
            app:layout_constraintTop_toBottomOf="@+id/textView5"
            tools:ignore="SpUsage" />

        <TextView
            android:id="@+id/myPage_textview_activitySub"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="70dp"
            android:fontFamily="@font/bmjua"
            android:text="마이페이지"
            android:textColor="@color/MainColor"
            android:textSize="24dp"
            app:layout_constraintStart_toEndOf="@+id/imageView7"
            app:layout_constraintTop_toBottomOf="@+id/userName_textview_drawer"
            tools:ignore="SpUsage" />

        <TextView
            android:id="@+id/notice_textview_activitySub"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="28dp"
            android:fontFamily="@font/bmjua"
            android:text="알림함"
            android:textColor="@color/MainColor"
            android:textSize="24dp"
            app:layout_constraintStart_toEndOf="@+id/imageView9"
            app:layout_constraintTop_toBottomOf="@+id/couponBox_textview_drawer"
            tools:ignore="SpUsage" />

        <TextView
            android:id="@+id/setting_textview_activitySub"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="28dp"
            android:fontFamily="@font/bmjua"
            android:text="환경설정"
            android:visibility="gone"
            android:textColor="@color/MainColor"
            android:textSize="24dp"
            app:layout_constraintStart_toEndOf="@+id/imageView10"
            app:layout_constraintTop_toBottomOf="@+id/notice_textview_activitySub"
            tools:ignore="SpUsage" />

        <TextView
            android:id="@+id/qna_textview_activitySub"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="28dp"
            android:fontFamily="@font/bmjua"
            android:text="문의하기"
            android:textColor="@color/MainColor"
            android:textSize="24dp"
            app:layout_constraintStart_toEndOf="@+id/imageView12"
            app:layout_constraintTop_toBottomOf="@+id/setting_textview_activitySub"
            tools:ignore="SpUsage" />

        <TextView
            android:id="@+id/logOut_textview_drawer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginBottom="32dp"
            android:fontFamily="@font/bmjua"
            android:text="로그아웃"
            android:textColor="@color/MainColor"
            android:textSize="24dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView14"
            tools:ignore="SpUsage" />

        <TextView
            android:id="@+id/couponBox_textview_drawer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="28dp"
            android:fontFamily="@font/bmjua"
            android:text="쿠폰함"
            android:textColor="@color/MainColor"
            android:textSize="24dp"
            android:visibility="visible"
            app:layout_constraintStart_toEndOf="@+id/imageView8"
            app:layout_constraintTop_toBottomOf="@+id/myPage_textview_activitySub" />

        <ImageView
            android:id="@+id/imageView7"
            android:tint="@color/MainColor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/myPage_textview_activitySub"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/myPage_textview_activitySub"
            app:srcCompat="@drawable/ic_person_pin_black_24dp" />

        <ImageView
            android:id="@+id/imageView8"
            android:tint="@color/MainColor"
            android:layout_width="25dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/couponBox_textview_drawer"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/couponBox_textview_drawer"
            app:srcCompat="@drawable/ic_coupon" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:tint="@color/MainColor"
            android:layout_marginStart="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/notice_textview_activitySub"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/notice_textview_activitySub"
            app:srcCompat="@drawable/ic_notification" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:tint="@color/MainColor"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="@+id/setting_textview_activitySub"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/setting_textview_activitySub"
            app:srcCompat="@drawable/ic_settings_black_24dp" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:tint="@color/MainColor"
            app:layout_constraintBottom_toBottomOf="@+id/qna_textview_activitySub"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/qna_textview_activitySub"
            app:srcCompat="@drawable/ic_raise_your_hand_to_ask" />

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:tint="@color/MainColor"
            app:layout_constraintBottom_toBottomOf="@+id/logOut_textview_drawer"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/logOut_textview_drawer"
            app:srcCompat="@drawable/ic_logout" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>